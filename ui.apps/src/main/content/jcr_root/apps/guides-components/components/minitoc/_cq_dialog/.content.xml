<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    xmlns:cq="http://www.day.com/jcr/cq/1.0"
    xmlns:jcr="http://www.jcp.org/jcr/1.0"
    xmlns:nt="http://www.jcp.org/jcr/nt/1.0" jcr:primaryType="nt:unstructured" jcr:title="Mini TOC" sling:resourceType="cq/gui/components/authoring/dialog">
    <content jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                <minitocTitle
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                    fieldDescription="Minitoc component title."
                    fieldLabel="Title"
                    value="On this page"
                    name="./title"/>
                    <minitocStart jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/select" emptyOption="{Boolean}true" fieldDescription="Minitoc heading range start'" fieldLabel="Start" name="./start" type="editable">
                        <items jcr:primaryType="nt:unstructured">
                            <h1 jcr:primaryType="nt:unstructured" text="h1" value="1"/>
                            <h2 jcr:primaryType="nt:unstructured" text="h2" value="2"/>
                            <h3 jcr:primaryType="nt:unstructured" text="h3" value="3"/>
                            <h4 jcr:primaryType="nt:unstructured" text="h4" value="4"/>
                            <h5 jcr:primaryType="nt:unstructured" text="h5" value="5"/>
                            <h6 jcr:primaryType="nt:unstructured" text="h6" value="6"/>
                        </items>
                    </minitocStart>
                    <minitocEnd jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/select" emptyOption="{Boolean}true" fieldDescription="Minitoc heading range end'" fieldLabel="End" name="./end" type="editable">
                        <items jcr:primaryType="nt:unstructured">
                            <h1 jcr:primaryType="nt:unstructured" text="h1" value="1"/>
                            <h2 jcr:primaryType="nt:unstructured" text="h2" value="2"/>
                            <h3 jcr:primaryType="nt:unstructured" text="h3" value="3"/>
                            <h4 jcr:primaryType="nt:unstructured" text="h4" value="4"/>
                            <h5 jcr:primaryType="nt:unstructured" text="h5" value="5"/>
                            <h6 jcr:primaryType="nt:unstructured" text="h6" value="6"/>
                        </items>
                    </minitocEnd>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
